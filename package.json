{
  "name": "lru-memorise",
  "version": "0.2.1",
  "description": "A simple memorise function that uses a fast LRU cache under the hood.",
  "keywords": [
    "memorise",
    "memorise function",
    "memorize",
    "lru memorise",
    "async memorise",
    "function cache"
  ],
  "type": "module",
  "module": "./dist/lib.js",
  "browser": "./dist/lib.browser.js",
  "types": "./dist/lib.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lib.js",
      "require": "./dist/lib.cjs",
      "types": "./dist/lib.d.ts"
    }
  },
  "license": "MIT",
  "author": "Chris A James <chris@webglowit.net>",
  "homepage": "https://github.com/cajames/lru-memorise",
  "bugs": {
    "url": "https://github.com/cajames/lru-memorise/issues"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "test": "jest --coverage",
    "build": "rm -r -f ./dist && NODE_ENV=production rollup --config rollup.config.js && rm -rf ./dist/types",
    "release": "yarn build && np"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "np": "^9.2.0",
    "rollup": "^4.12.0",
    "rollup-plugin-dts": "^6.1.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "tiny-lru": "^11.2.5"
  },
  "wallaby": {
    "autoDetect": true
  },
  "resolutions": {}
}
